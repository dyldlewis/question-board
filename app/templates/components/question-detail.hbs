<button {{action 'delete' question}} class="btn btn-danger" class="delete">Delete</button>
<h3>{{quetion.author}}</h3>
<h3 class="content">{{question.content}}</h3>
<h5>{{question.notes}}</h5>


<h2> Answers </h2>
<ul>
  {{#each question.answers as |answer|}}
    {{answer-tile answer=answer}}
  {{/each}}
</ul>
